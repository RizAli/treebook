<div>
  <div class="span4">


    <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "well"}) do |f| %>
      <%= devise_error_messages! %>

      <fieldset>
        <legend>Sign up</legend>
        <div><%= f.input :first_name %><br />
         </div>

        <div><%= f.input :last_name %><br />
         </div>

        <div><%= f.input :profile_name %><br />


        <div class="field">
          <%= f.input :email, autofocus: true %><br />
        </div>

        <div class="field">
          <%= f.input :password, autocomplete: 'off'%>
          <% if @validatable %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
        </div>

        <div class="field">
          <%= f.input :password_confirmation, autocomplete: "off" %><br />
        </div>
      </fieldset>
      <div class="actions">
        <%= f.submit "Sign up" %>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
  </div>
</div>